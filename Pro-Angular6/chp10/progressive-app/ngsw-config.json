{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, 
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/fonts/*"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-categories",
      "urls": ["http://ojlinks-api.test:8084/api/categories"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "5d"
      }
    }
  ],
  "navigationUrls": [
    "/**"
  ]
}